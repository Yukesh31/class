var obj = {
  firstName: "John",
  lastName: "Doe",

  address: {
    street: "Main Street",
    city: "New York",
    state: "NY",
    zip: "10001",
  },
  getName: function () {
    return this.firstName + " " + this.lastName;
  },

  getAddress: function () {
    return this.zip;
  },
};

console.log(obj.getName());
console.log(obj.getAddress());

