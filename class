{
  name: 'Joey',
  age: 3,
  type: 'dog'
},
];

let result = (function (array) {
  return (array.filter((odd) => {
  if(odd.type=="dog")
  return odd;
  }))
  })(array);
  console.log( result);

  let result1= (function (result) {
    return (result.map((item)=>
    {
     return (item.age*5);
   }))
  })
  (result);
  console.log( result1);

  let result2=(function (result1) {
    return (result1.reduce((total,item)=>
    {
  return  total+item;
   
  }))
  })
  (result1);
  
  console.log( result2);
  
  
