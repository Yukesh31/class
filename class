fetch("https://restcountries.eu/rest/v2/all")
.then((resp)=>
{
  return resp.json();
})
.then((result)=>{
  var t=result;
  for(i=0;i<t.length;i++)
  console.log(t[i].name,t[i].flag);

})
.catch((err)=>{
  console.log(err)

});
